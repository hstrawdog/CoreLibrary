<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hqq.album">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name="com.hqq.album.activity.AlbumFolderActivity"
            android:theme="@style/NoTitle" />
        <activity
            android:name="com.hqq.album.activity.AlbumDirectoryActivity"
            android:theme="@style/NoTitle" />
        <activity
            android:name="com.hqq.album.activity.AlbumDetailActivity"
            android:theme="@style/NoTitle" />
        <activity
            android:name="com.hqq.album.activity.AlbumPreviewActivity"
            android:theme="@style/NoTitle" />
        <activity
            android:name="com.hqq.album.activity.PreviewActivity"
            android:theme="@style/NoTitle" />
    </application>

</manifest>